\NeedsTeXFormat{LaTeX2e}[1994/06/01]
\ProvidesPackage{my-tikz-clipart}[2023/07/20 Custom Package]
\RequirePackage{tikz}
\usetikzlibrary{
  tikzlings,
  patterns,
  shadings,
  shapes.geometric
}

\definecolor{mygreen}{HTML}{92C079}
\definecolor{mydarkgreen}{HTML}{3F5233}
\tikzset{
  car/.pic = {
    \filldraw[top color=red!50, draw=black, rounded corners] 
      (0,0) -- (0,1) -- (4.5,1) -- 
      (5.5,.6) -- (5.5,0) -- cycle;
    \draw[fill=blue!10] 
      (1,1) -- (1.5,1.75) -- (3,1.75) -- 
      (4,1) -- cycle;
    \draw[fill=gray!50, draw=black, ultra thick] 
      (1,0) circle (.5);
    \draw[fill=gray!50, draw=black, ultra thick] 
      (4.5,0) circle (.5);
  },
  truck/.pic = {
    \filldraw[top color=orange!50, draw=black, rounded corners] 
      (0,0) -- (0,1.25) --  (2,1.25) -- (4.5,1.25) -- 
      (5.5,.6) -- (5.5,0) -- cycle;
    \draw[fill=blue!10] 
      (2.5,1.25) -- (2.5,2.5) -- (4,2.5) -- 
      (4.5,1.25) -- cycle;
    \draw[fill=gray!50, draw=black, ultra thick] 
      (1,0) circle (.5);
    \draw[fill=gray!50, draw=black, ultra thick] 
      (4.5,0) circle (.5);
  },
  bucket/.pic = {
    \node[
        cylinder,draw,
        rotate=90,
        minimum width=1.5cm,
        minimum height=1.5cm,
        cylinder uses custom fill, 
        cylinder end fill=blue!50,
        cylinder body fill=blue!25
      ] at (0,0) {};
    \draw[ultra thick, blue] (.75,.75) arc[
        start angle = 0,
        end angle = 180,
        x radius=.75cm,
        y radius=1cm
     ];
  },
  bike/.pic = {
    \begin{scope}[
      axle/.style={fill=mydarkgreen},
      cb/.style={
        rounded corners,
          pattern=crosshatch,
          pattern color=mygreen
        },
      draw=mygreen, 
      very thick
    ]
      %back wheel
      \coordinate (backwheel) at (-1.5,0);
      \draw (backwheel) circle (1cm);
      \draw (backwheel) circle (0.9cm);
      \draw[axle] (backwheel) circle (1mm);
  
  
      %front wheel
      \coordinate (frontwheel) at (1.75,0);
      \draw (frontwheel) circle (1cm);
      \draw (frontwheel) circle (0.9cm);
      \draw[axle] (frontwheel) circle (1mm);
  
  
      \coordinate (pedal) at (0,-.2);
      \draw (pedal) circle (1mm);
  
      \coordinate (seat) at (-.5,1.5);
      \draw[cb] (seat) ++ (0,-.05) -- + (.25,.25) -- + (-.25,.25) -- cycle;
  
      \coordinate (handlebars) at (1,1.5);
      \draw[ultra thick] (handlebars) -- ++ (-.2,.3) + (-.4,-.4) -- + (.4,.4);
  
  
      \draw (backwheel) -- (pedal) -- (seat) -- 
        (backwheel);
      \draw (pedal) -- (handlebars) -- (seat);
      \draw (handlebars) -- (frontwheel);
    \end{scope}
  }
}

