      \draw[ultra thin, draw=gray!50] 
        (-3,1) -- + (-1.5,0) 
        ++ (-.5,-.3) -- + (-1.25,0) 
        ++ (-.1,-.3) -- + (-.8,0);
    \end{tikzpicture}
  \end{center}
}


